{
  "expo": {
    "name": "Tazkmate",
    "slug": "tazkmate",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/tazkmate-icon-1024.png",
    "scheme": "tazkmate",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/tazkmate-adaptive-foreground-1024.png",
        "backgroundColor": "#0EA5E9"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.huendrys.tazkmate"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/tazkmate-icon-1024.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "74056687-8f2c-4de0-8e70-ca95235ae3c2"
      }
    },
    "owner": "huendrys"
  }
}
